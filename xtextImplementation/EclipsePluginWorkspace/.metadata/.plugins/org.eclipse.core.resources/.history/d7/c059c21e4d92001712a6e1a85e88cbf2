Computer c1{
	incoming(gfc5)
}
Computer c2{
	incoming(gfc6)
}
Server s1{
	maxSlots 5
	MTBF 20
	outgoing(gfc1)
}
Server s2{
	maxSlots 5
	MTBF 20
	outgoing(gfc2)
}
Router r1{ 
	maxSpeed 10
	incoming(gfc3)
	outgoing(gfc5)
}
Router r2{
	maxSpeed 10
	incoming(gfc4)
	outgoing(gfc6)
}
Router r3{  
	maxSpeed 10
	incoming(gfc1, gfc2)
	outgoing(gfc3, gfc4)
}

GlassFiberCable gfc1{
	speed 10
	endPoint1 s1
	endPoint2 r3
	isDuplex false
}

GlassFiberCable gfc2{
	speed 10
	endPoint1 s2
	endPoint2 r3
	isDuplex false
}

GlassFiberCable gfc3{
	speed 10
	endPoint1 r3
	endPoint2 r1
	isDuplex false
}

GlassFiberCable gfc4{
	speed 10
	endPoint1 r3
	endPoint2 r2
	isDuplex false
}

